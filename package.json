{
    "name": "workit-ecommerce",
    "private": true,
    "scripts": {
        "dev": "npx pnpm turbo dev",
        "build": "npx pnpm turbo build",
        "lint": "npx pnpm turbo lint",
        "start": "npx pnpm turbo start",
        "db:studio": "pnpm --filter @workit/db studio",
        "studio": "pnpm --filter @workit/db studio"
    },
    "devDependencies": {
        "turbo": "latest"
    },
    "packageManager": "pnpm@10.28.2",
    "pnpm": {
        "overrides": {
            "unrs-resolver": "npm:is-buffer@^2.0.5"
        },
        "onlyBuiltDependencies": [
            "esbuild",
            "sharp"
        ]
    }
}